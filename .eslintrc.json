{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "prefer-const": "error",
        "no-var": "error",
        "eqeqeq": "error",
        "no-multiple-empty-lines": ["error", { "max": 2 }]
    },
    "globals": {
        "window": "readonly",
        "document": "readonly",
        "console": "readonly",
        "performance": "readonly",
        "navigator": "readonly",
        "CSS": "readonly"
    }
}
